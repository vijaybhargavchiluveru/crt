'''
n=int(input())
p=n
for i in range(1,n*2):
    for j in range(1,n+2):
        if(j<=p):
            print("*",end=' ')
        else:
            print("@",end=' ')
    print()
    if(i<n):
        p-=1
    else:
        p+=1
        
