#2.program
'''
n=int(input())
p=n
q=n+2
for i in range(1,n*2):
    for j in range(1,n*2+2):
        if(j<=p or j>=q):
            print("*",end=' ')
        else:
            print("@",end=' ')
    print()
    if(i<n):
        p-=1
        q+=1
    else:
        p+=1
        q-=1

n=4
p=1
q=9
for i in range(1,n*2):
    for j in range(1,n*2+2):
        if(j>p and j<q):
            print("@",end=' ')
        else:
            print("*",end=' ')
    print()
    if(i<n):
        p+=1
        q-=1
    else:
        p-=1
        q+=1

#4
n=int(input())
p=n
q=n
for i in range(1,n*2):
    for j in range(1,n*2):
        if(j<p or j>q):
            print(' ',end=' ')
        else:
            print('*',end=' ')
    print()
    if(i<n):
        p-=1
        q+=1
    else:
        p+=1
        q-=1
