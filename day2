24.
n=int(input())
for i in range(1, n//2+2):
    for j in range(n//2+1-i):
        print(" ", end="")
    for k in range(2*i-1):
        print("*", end="")
    print()
for i in range(n//2, 0, -1):
    for j in range(n//2+1-i):
        print(" ", end="")
    for k in range(2*i-1):
        print("*", end="")
    print()
print()

25.
n=int(input())
for i in range(1, n//2+2):
    for j in range(n//2+1-i):
        print(" ", end="")
    for k in range(2*i-1):
        if i == 3 and k >= 4 and k <= 12:
            if k == 4:
                print("C", end="")
            elif k == 5:
                print("o", end="")
            elif k == 6:
                print("d", end="")
            elif k == 7:
                print("e", end="")
            elif k == 8:
                print("F", end="")
            elif k == 9:
                print("o", end="")
            elif k == 10:
                print("r", end="")
            elif k == 11:
                print("w", end="")
            elif k == 12:
                print("i", end="")
            else:
                print("*", end="")
        else:
            print("*", end="")
    print()
for i in range(n//2, 0, -1):
    for j in range(n//2+1-i):
        print(" ", end="")
    for k in range(2*i-1):
        print("*", end="")
    print()
print()

26.
n=int(input())
for i in range(1, n):
    for j in range(n-i):
        print(" ", end="")
    for k in range(i):
        print("*", end="")
    print()
for i in range(n, 0, -1):
    for j in range(n-i):
        print(" ", end="")
    for k in range(i):
        print("*", end="")
    print()
print()

27.
n=int(input())
mid = n // 2
for i in range(n):
    for j in range(n):
        if i == mid or j == mid:
            print("+", end="")
        else:
            print(" ", end="")
    print()
print()

28.
n=int(input())
for i in range(n):
    for j in range(n):
        if i == j or i + j == n - 1:
            print("*", end=" ")
        else:
            print(" ", end=" ")
    print()
print()

29.
for i in range(7):
    if i == 0 or i == 3 or i == 6:
        print("*")
    else:
        print("* *")
print()
