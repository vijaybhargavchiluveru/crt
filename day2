star patterns

1.
n = int(input())
for i in range(n):
for j in range(n):
print('*')

2.

n = int(input())
for i in range(n):
for j in range(n):
if(i==0 or i ==(n-1) or j ==0 or j ==(n-1):
print('*')

3.
n = int(input())
for i in range(n):
for j in range(n):
if(i==1 or i==n or j==1 or j==n or i==j or j==(n - i + 1)):
print('*')
else:
print(' ')

4.
n=int(input())
k = 4
for i in range(n):
print(' '*k)
for j in range(n):
print('*');
k-=1

5.
n=int(input())
for i in range(n):
    for j in range(n):
        if i == 0 or i == n-1 or j == 0 or j == n-1 or i == j:
            print("*", end=" ")
        else:
            print(" ", end=" ")
    print()
print()

6.
n=int(input())
for i in range(n):
    for j in range(n-i-1):
        print(" ", end="")
    for k in range(2*i+1):
        print("*", end="")
    print()
for i in range(n-2, -1, -1):
    for j in range(n-i-1):
        print(" ", end="")
    for k in range(2*i+1):
        print("*", end="")
    print()
print()

7.
n=int(input())
for i in range(n):
    for j in range(n-i-1):
        print(" ", end="")
    for k in range(2*i+1):
        if k == 0 or k == 2*i or i == 0:
            print("*", end="")
        else:
            print(" ", end="")
    print()
for i in range(n-2, -1, -1):
    for j in range(n-i-1):
        print(" ", end="")
    for k in range(2*i+1):
        if k == 0 or k == 2*i or i == 0:
            print("*", end="")
        else:
            print(" ", end="")
    print()
print()

8.
n=int(input())
for i in range(n):
    for j in range(i):
        print(" ", end="")
    for k in range(2*(n-i)-1):
        print("*", end="")
    print()
for i in range(n-2, -1, -1):
    for j in range(i):
        print(" ", end="")
    for k in range(2*(n-i)-1):
        print("*", end="")
    print()
print()

9.
n=int(input())
for i in range(n):
    for j in range(i):
        print(" ", end="")
    for k in range(2*(n-i)-1):
        if k == 0 or k == 2*(n-i)-2 or i == 0:
            print("*", end="")
        else:
            print(" ", end="")
    print()
for i in range(n-2, -1, -1):
    for j in range(i):
        print(" ", end="")
    for k in range(2*(n-i)-1):
        if k == 0 or k == 2*(n-i)-2 or i == 0:
            print("*", end="")
        else:
            print(" ", end="")
    print()
print()

